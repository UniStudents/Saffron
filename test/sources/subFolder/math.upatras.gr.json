{
  "url": "https://www.math.upatras.gr/el/news/announcements/",
  "name": "math.upatras.gr",
  "type": "html",
  "endpoint": "math.upatras.gr",
  "scrape": {
    "container": "body > main > div > div > div > ul > li",
    "article": {
      "link": {
        "class": ".title",
        "find": [
          "a"
        ],
        "attributes": [
          "href"
        ],
        "multiple": false
      },
      "pubDate": {
        "class": "p:nth-child(3)",
        "find": null,
        "multiple": false
      },
      "title": {
        "class": ".title > a",
        "find": null,
        "multiple": false
      },
      "content": {
        "class": "p:nth-child(4)",
        "find": null,
        "multiple": false
      }
    }
  }
}
