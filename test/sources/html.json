{
  "url": [
    ["unipi","https://www.unipi.gr/unipi/el/%CE%B1%CE%BD%CE%B1%CE%BA%CE%BF%CE%B9%CE%BD%CF%8E%CF%83%CE%B5%CE%B9%CF%82.html"]
  ],
  "name": "html-cs.unipi.gr",
  "type": "html",
  "retryInterval": 30000,
  "scrapeInterval": 60000,
  "scrape": {
    "container": ".catItemView",
    "endPoint": "unipi.gr",
    "article": {
      "pubDate": {
        "class": ".catItemDateCreated",
        "attributes": [
          "value",
          "href"
        ],
        "find": null,
        "multiple": false
      },
      "title": {
        "class": ".catItemTitle",
        "find": [
          "a"
        ],
        "multiple": false
      },
      "content": {
        "class": ".catItemBody",
        "find": null,
        "multiple": false
      },
      "links": {
        "class": ".catItemLinks",
        "attributes": [
          "value",
          "href"
        ],
        "find": [
          ".catItemAttachmentsBlock",
          "li",
          "a"
        ],
        "multiple": true
      }
    }
  }
}